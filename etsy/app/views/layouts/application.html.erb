<!DOCTYPE html>
<html>
    <head>
        <title>Etsy</title>
        <%= csrf_meta_tags %>

        <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
        <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    </head>

    <body>
        <header>
            <%= link_to "About", about_path %>
            <%= link_to "Contact", contact_path %>
        </header>
        <%= yield %>
        <footer>
            <p>Copyright
                <%= Date.today.year %></p>
        </footer>
    </body>
</html>
